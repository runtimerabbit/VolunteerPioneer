{
  "expo": {
    "name": "Volunteer Pioneer",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Volunteer Pioneer to use your location?"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/m-plus-rounded-1c/MPLUSRounded1c_700Bold.ttf"
          ]
        }
      ]
    ],
    "slug": "volunteer-pioneer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.company",
      "config": {
        "googleMapsApiKey": "AIzaSyCoveCCHopXULsHaBKuG44cwYiDl_v-1-8"
      },
      "infoPlist": {
      "NSAppTransportSecurity": { 
        "NSAllowsArbitraryLoads": true
        }
      }
    },
    "build": {
      "production": {
        "env": {
          "EXPO_PUBLIC_API_URL": "https://volunteerpioneer.onrender.com/",
          "GOOGLE_API_KEY": "AIzaSyBU5Har5aYmd-FIABD5dC33TSmZ57td61I"
        }
      }
    },
    "android": {
      "package": "com.company",
      "config": {
      "googleMaps": {
          "apiKey": "AIzaSyCoveCCHopXULsHaBKuG44cwYiDl_v-1-8"
        }
      },
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "2cea540b-4f76-45a9-86ca-b9e1ef371fb5"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2cea540b-4f76-45a9-86ca-b9e1ef371fb5"
    }
  }
}
